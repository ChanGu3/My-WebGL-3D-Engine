{
  "name": "app",
  "version": "1.0.0",
  "description": "for building tailwind css file and compiling typescript into javascript",
  "main": "",
  "scripts": {
    "dev-css": "npx @tailwindcss/cli -i ./src/tailwind.css -o ./dist/index.css --watch",
    "dev-ts": "npx tsc --watch",
    "dev-all": "concurrently \"npm run dev-css\" \"npm run dev-ts\"",
    "dev": "npm run dev-all"
  },
  "keywords": [],
  "author": "Chandler A Guthrie",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.12",
    "concurrently": "^9.2.0",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2"
  }
}
